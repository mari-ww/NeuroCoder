    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#6A9955"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        }
    ]
}